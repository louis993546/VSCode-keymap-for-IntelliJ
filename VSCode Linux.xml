<keymap version="1" name="VSCode Linux">
  <action id="ActivateDebugToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl d" />
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl e" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
  <action id="ActivateVersionControlToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl g" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt minus" />
  </action>
  <action id="EditorBackSpace">
    <keyboard-shortcut first-keystroke="back_space" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="shift ctrl k" />
  </action>
  <action id="EditorEnter">
    <keyboard-shortcut first-keystroke="enter" />
  </action>
  <action id="EditorEscape">
    <keyboard-shortcut first-keystroke="escape" />
  </action>
  <action id="EditorPageDown">
    <keyboard-shortcut first-keystroke="page_down" />
  </action>
  <action id="EditorPageUp">
    <keyboard-shortcut first-keystroke="page_up" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
  </action>
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="shift ctrl p" />
    <keyboard-shortcut first-keystroke="f1" />
  </action>
  <action id="MoveLineDown">
    <keyboard-shortcut first-keystroke="alt down" />
  </action>
  <action id="MoveLineUp">
    <keyboard-shortcut first-keystroke="alt up" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="shift ctrl i" />
  </action>
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="z" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="f11" />
  </action>
</keymap>